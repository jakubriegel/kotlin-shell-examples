<!-- used when ran by intellij -->

<Configuration status="WARN" name="kotlinshellextension" packages="">
    <Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} %p %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="ALL">
            <AppenderRef ref="stdout"/>
        </Root>
    </Loggers>
</Configuration>
